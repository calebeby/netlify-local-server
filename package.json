{
  "name": "netlify-local-server",
  "license": "MIT",
  "bin": {
    "netlify-local-server": "./cli.js"
  },
  "devDependencies": {
    "@cloudfour/eslint-config": "1.0.0",
    "@types/browser-sync": "0.0.41",
    "@types/clipboardy": "1.1.0",
    "@types/connect": "3.4.32",
    "@types/jest": "23.3.1",
    "@types/mime": "2.0.0",
    "@types/node": "10.5.7",
    "@types/supertest": "2.0.5",
    "browser-sync": "2.24.6",
    "connect": "3.6.6",
    "eslint": "5.3.0",
    "eslint-formatter-pretty": "1.3.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jest": "21.20.2",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-standard": "3.1.0",
    "eslint-plugin-typescript": "0.12.0",
    "eslint-plugin-unicorn": "5.0.0",
    "jest": "23.5.0",
    "prettier": "1.14.2",
    "supertest": "3.1.0",
    "ts-jest": "23.1.3",
    "typescript": "3.0.1",
    "typescript-eslint-parser": "18.0.0"
  },
  "scripts": {
    "start": "tsc --watch",
    "build": "tsc",
    "test": "jest",
    "check-lint": "prettier --list-different '**/*.js' '**/*.ts' && eslint '**/*.js' '**/*.ts' --format pretty",
    "lint": "prettier --write '**/*.js' '**/*.ts' && eslint --fix '**/*.js' '**/*.ts' --format pretty"
  },
  "eslintConfig": {
    "extends": [
      "@cloudfour/eslint-config",
      "@cloudfour/eslint-config/typescript",
      "plugin:jest/recommended"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "clipboardy": "1.2.3",
    "kleur": "2.0.1",
    "mime": "2.3.1"
  }
}
